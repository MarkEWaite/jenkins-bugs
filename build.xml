<project name="JENKINS-33433" default="increment" basedir=".">

  <!-- Increment build number, commit the change -->
  <target name="increment" description="Show JENKINS-33433 is not repeatable">
    <echo>java is ${java.version}</echo>
    <buildnumber/>
    <echo>Build number was ${build.number}</echo>
    <exec executable="git">
      <arg value="add"/>
      <arg value="build.number"/>
      <arg value="."/>
    </exec>
    <exec executable="git">
      <arg value="commit"/>
      <arg value="-m"/>
      <arg value="${user.name} on JENKINS-33433, build was ${build.number}"/>
      <arg value="build.number"/>
    </exec>
    <exec executable="git">
      <arg value="merge"/>
      <arg value="-s"/>
      <arg value="octopus"/>
      <arg value="JENKINS-33433-branch-1"/>
      <arg value="JENKINS-33433-branch-2"/>
    </exec>
  </target>
  </target>

</project>
